<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Deletion_Forbidden</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Status__c , &quot;Completed&quot;), ISNULL( Completed_Date__c  ))</errorConditionFormula>
    <errorDisplayField>Status__c</errorDisplayField>
    <errorMessage>Requests cannot be deleted once it has been marked as completed.</errorMessage>
</ValidationRule>
