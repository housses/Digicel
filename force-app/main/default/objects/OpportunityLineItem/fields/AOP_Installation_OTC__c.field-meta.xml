<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AOP_Installation_OTC__c</fullName>
    <description>IF( ISNULL( Opportunity.Estimated_Billed_Date__c ) ,0,

* CASE( MONTH(Opportunity.Estimated_Billed_Date__c ) , 
1,3, 
2,2, 
3,1, 
16-MONTH( Opportunity.Estimated_Billed_Date__c ))) ,0))</description>
