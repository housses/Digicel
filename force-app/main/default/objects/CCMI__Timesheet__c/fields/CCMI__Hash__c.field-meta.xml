<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CCMI__Hash__c</fullName>
    <deprecated>false</deprecated>
    <description>This is a hash of the OwnerId of the Timesheet and the start date, it is used to enforce unique timesheets</description>
    <externalId>false</externalId>
    <formula>CASESAFEID(OwnerId) + TEXT(YEAR(CCMI__Start__c)) + &apos;-&apos; + TEXT(MONTH(CCMI__Start__c)) + &apos;-&apos; + TEXT(DAY(CCMI__Start__c))</formula>
    <inlineHelpText>This is a system field and should never be visible.</inlineHelpText>
    <label>Hash</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
