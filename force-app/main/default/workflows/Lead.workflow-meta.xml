    <rules>
        <fullName>Lead Assigned</fullName>
        <actions>
            <name>Lead_Status_Set_to_Assigned</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Used to change the status of web leads when they are assigned to a user</description>
        <formula>ISCHANGED(OwnerId) &amp;&amp; (RecordTypeId = &quot;012d0000001xkq5&quot; || RecordTypeId = &quot;0120W000001lF7s&quot;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
