    <rules>
        <fullName>Email to Customer</fullName>
        <actions>
            <name>Send_email_to_Lead_Contact</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Lead.Country__c</field>
            <operation>equals</operation>
            <value>Jamaica</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.RecordTypeId</field>
            <operation>equals</operation>
            <value>B2B Sales Lead,Business Solutions W2L</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
