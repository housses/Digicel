<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>All_Digicel_Case_Read_Write_Access</fullName>
        <accessLevel>Edit</accessLevel>
        <description>This rule grants read access to any case on the SFDC admin ticket record type</description>
        <label>All Digicel Case Read Write Access</label>
        <sharedTo>
            <roleAndSubordinates>Chairman_s_Committee</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>SFDC Admin Ticket</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>PNG_AccessToSalesOpsBillingGroup</fullName>
        <accessLevel>Edit</accessLevel>
        <label>PNG Access To Sales Ops &amp; Billing Group</label>
        <sharedTo>
            <group>PNG_SalesOps_BillingGroup</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Digicel PNG Implementation Case</value>
        </criteriaItems>
        <criteriaItems>
            <field>Status</field>
            <operation>notEqual</operation>
            <value>Closed</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>PNG_Case_Sharing_Roles_And_Subs_PNG</fullName>
        <accessLevel>Read</accessLevel>
        <label>PNG Case Sharing Roles And Subs PNG</label>
        <sharedTo>
            <roleAndSubordinates>PNG_Sales_Manager</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Digicel PNG Support Case</value>
        </criteriaItems>
        <criteriaItems>
            <field>Status</field>
            <operation>notEqual</operation>
            <value>Closed,Merged</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>PNG_Case_Sharing_Roles_And_Subs_PNG_PreSales</fullName>
        <accessLevel>Edit</accessLevel>
        <label>PNG Case Sharing Roles And Subs PNG PreSales</label>
        <sharedTo>
            <roleAndSubordinates>PNG_Pre_Sales</roleAndSubordinates>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Digicel PNG Support Case</value>
        </criteriaItems>
        <criteriaItems>
            <field>IsClosed</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>PNG_Case_Sharing_Roles_And_Subs_PNG_Service_Manager</fullName>
        <accessLevel>Edit</accessLevel>
        <label>PNG Case Sharing Roles And Subs PNG Service Manager</label>
        <sharedTo>
            <roleAndSubordinates>PNG_Service_Manager</roleAndSubordinates>
        </sharedTo>
        <booleanFilter>1 AND 2 AND (3 OR 4)</booleanFilter>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Digicel PNG Support Case</value>
        </criteriaItems>
        <criteriaItems>
            <field>IsClosed</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>startsWith</operation>
            <value>00G0</value>
        </criteriaItems>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>startsWith</operation>
            <value>0050</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>false</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>PNG_ICT_Support_TL</fullName>
