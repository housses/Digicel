<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>amc__Cost_Schedule__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>if( ISPICKVAL(amc__Project_Day_Rate__c, &quot;Standard&quot;),  amc__Billable_Hourly_Rate__c  * amc__Hours_Scheduled__c,  amc__Hours_Scheduled_Cost__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Provides a summary for the Hours Scheduled - Billable. (Previously Labelled Cost Schedule)</inlineHelpText>
    <label>Billable Schedule</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
