<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CCMI__Completion_Percent__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(CCMI__Status__c, &apos;Completed&apos;),
  1,
  IF(ISBLANK(CCMI__Total_Sub_Tasks__c) || CCMI__Total_Sub_Tasks__c == 0,
    0,
    CCMI__Total_Completed_Sub_Tasks__c / CCMI__Total_Sub_Tasks__c
  )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Completion Percent</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
