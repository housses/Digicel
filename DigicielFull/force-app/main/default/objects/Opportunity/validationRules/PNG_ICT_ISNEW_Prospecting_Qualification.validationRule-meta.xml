<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PNG_ICT_ISNEW_Prospecting_Qualification</fullName>
    <active>true</active>
    <errorConditionFormula>IF($RecordType.Id=&quot;012d0000000sds8&quot;&amp;&amp;ISNEW(),
NOT(ISPICKVAL( StageName, &quot;Prospecting &amp; Qualification&quot;))
,FALSE)</errorConditionFormula>
    <errorDisplayField>StageName</errorDisplayField>
    <errorMessage>New opportunity created cannot have a different stage than Prospecting &amp; Qualification.</errorMessage>
</ValidationRule>
