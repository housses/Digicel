<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>B2B_CALA_Bill_Date_Greater_Than_Close</fullName>
    <active>true</active>
    <description>Rule to prevent estimated bill date to be less than close date</description>
    <errorConditionFormula>Estimated_Billed_Date__c &lt; CloseDate &amp;&amp;  Digicel_Country__c &lt;&gt;&quot;PNG&quot;

&amp;&amp; $Profile.Name &lt;&gt;&quot;System Administrator&quot;</errorConditionFormula>
    <errorDisplayField>Estimated_Billed_Date__c</errorDisplayField>
    <errorMessage>Estimated Bill Must Be Greater Than Close Date</errorMessage>
</ValidationRule>
