<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Trinidad_cannot_change_the_ownership</fullName>
    <active>false</active>
    <errorConditionFormula>IF(NOT(ISNEW()),

IF(BEGINS(RecordType.DeveloperName, &quot;Trinidad&quot;)  &amp;&amp;  ($Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp;  $User.User_Id__c  &lt;&gt; &quot;005d0000004mVUP&quot; &amp;&amp; $User.User_Id__c &lt;&gt; &quot;005d0000002lKdd&quot; ),

AND(ISCHANGED(OwnerId) )
,
FALSE)
,FALSE)</errorConditionFormula>
    <errorMessage>You cannot change the owner of the Presales Request</errorMessage>
</ValidationRule>
