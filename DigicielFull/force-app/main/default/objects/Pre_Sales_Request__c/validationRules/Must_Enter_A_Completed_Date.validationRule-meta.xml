<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_Enter_A_Completed_Date</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(Status__c , &quot;Complete&quot;), ISNULL( Completed_Date__c  ))</errorConditionFormula>
    <errorDisplayField>Completed_Date__c</errorDisplayField>
    <errorMessage>You Must enter a Completed Date</errorMessage>
</ValidationRule>
