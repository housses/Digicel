<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>amc__Projected_Value_Cost__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(amc__Completion_Percent__c), amc__Hours_Remaining_Value_Cost__c, ((amc__Projected_Hours_Remaining_Billable__c + amc__Projected_Hours_Remaining_Non_Billable__c) * amc__Cost_Hourly_Rate__c) + amc__Hours_Completed_Value_Cost__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Identifies the projected cost value for this action. If Completion % is blank, Hours Remaining Value (Cost), otherwise ((Projected hours Remaining Billable + Projected Hours Remaining Non Billable) * Cost Hourly Rate) + Hours Completed Value (Cost)</inlineHelpText>
    <label>Projected Value (Cost)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
