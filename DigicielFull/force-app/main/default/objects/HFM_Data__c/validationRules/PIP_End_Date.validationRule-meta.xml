<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PIP_End_Date</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    ISPICKVAL( Employee_Status__c , &apos;On PIP&apos;),
    ISBLANK(  PIP_End_Date__c  )
)</errorConditionFormula>
    <errorDisplayField>PIP_End_Date__c</errorDisplayField>
    <errorMessage>Please populate PIP End Date</errorMessage>
</ValidationRule>
